//Texte schmal und weiter screen Positionieren
function change_txt1_wide(){
  TweenLite.set(txt_1_1,{left: -10, top: 0});
  TweenLite.set(txt_1_2,{left: 200,top: 15});
  TweenLite.set(txt_1_3,{left: -20,top: 140});
  TweenLite.set(txt_1_4,{left: 90,top: 110});
}
function change_txt_1(){
  TweenLite.set(txt_1_1,{ left: 0, top: 0});
  TweenLite.set(txt_1_2,{ left: 0, top: 135});
  TweenLite.set(txt_1_3,{left: 110, top: 125});
  TweenLite.set(txt_1_4,{left: 10, top: 225});
}

//RESETTEN
function resetAll(){
    active=2;
    //
    TweenLite.set(bannerClassArray,{force3D:true});
        TweenLite.set(banner,{rotation:0,transformOrigin:"0% 0%",force3D:true});
    //Motive
    TweenLite.set(cta_container,{opacity:1,transformOrigin:"50% 50%",scale:1,force3D:true});
        TweenLite.set(cta_wrapper,{x:0,y:0, opacity:0,scale:4,rotation:0,transformOrigin:"50% 50%",force3D:true});
            TweenLite.set(cta,{x:0,y:0,opacity:1,scale:1,rotation:0,transformOrigin:"50% 50%",force3D:true});
    //Motive
    TweenLite.set(ile_container,{opacity:1,transformOrigin:"50% 50%",scale:1,force3D:true});
        TweenLite.set(ile_wrapper,{x:0,y:0, opacity:0,scale:1,rotation:0,transformOrigin:"50% 50%",force3D:true});
            TweenLite.set(ile,{x:0,y:0,opacity:1,scale:1,rotation:0,transformOrigin:"50% 50%",force3D:true});
   //Stoerer
    TweenLite.set(stoerer_container,{opacity:1,transformOrigin:"50% 50%",scale:1,force3D:true});
        TweenLite.set(stoerer_wrapper,{x:0,y:0, opacity:0,scale:4,rotation:0,transformOrigin:"50% 50%",force3D:true});
            TweenLite.set(stoerer,{x:0,y:0,opacity:1,scale:1,rotation:0,transformOrigin:"50% 50%",force3D:true});
    //Burger
    TweenLite.set(burger_wrapper,{ opacity:1,rotation:0,transformOrigin:"50% 50%",force3D:true});
        TweenLite.set(burger,{ x:500, y:0, opacity:0, scale:1, rotation:0, transformOrigin:"50% 50%", force3D:true});
    //TXT1
    TweenLite.set(txt_1_container,{opacity:1, scale:1, transformOrigin:"50% 50%",force3D:true});
        TweenLite.set(txt_1_wrapper,{opacity:1, x:0, scale:1, transformOrigin:"50% 50%",force3D:true});
            TweenLite.set(txt_1_1,{x:0, y:0, opacity:0, scale:0.4,rotation:0,transformOrigin:"50% 50%",force3D:true});
            TweenLite.set(txt_1_2,{x:0, y:0, opacity:0, scale:0.4,rotation:0,transformOrigin:"50% 50%",force3D:true});
            TweenLite.set(txt_1_3,{x:0, y:0, opacity:0, scale:0.4,rotation:0,transformOrigin:"50% 50%",force3D:true});
            TweenLite.set(txt_1_4,{x:0, y:0, opacity:0, scale:0.4,rotation:0,transformOrigin:"50% 50%",force3D:true});
    //TXT1
    //LOGO
    TweenLite.set(logo_container,{opacity:1,transformOrigin:"100% 0%",force3D:true});
        TweenLite.set(logo,{opacity:1,force3D:true});

        TweenLite.set(legal_2, {opacity:1} );
}

//ANIMATIONEN
function animate_0(){
    //
    TweenLite.to( txt_1_1, 1.3, { x:0, y:0, opacity:1, scale:1, rotation:0.02, ease: Elastic.easeOut.config(0.8, 0.3), delay:0.8 });
    TweenLite.to( txt_1_2, 1.3, { x:0, y:0, opacity:1, scale:1, rotation:0.02, ease: Elastic.easeOut.config(0.8, 0.3), delay:1 });
    TweenLite.to( txt_1_3, 1.3, { x:0, y:0, opacity:1, scale:1, rotation:0.02, ease: Elastic.easeOut.config(0.8, 0.3), delay:1.2 });
    TweenLite.to( txt_1_4, 1.3, { x:0, y:0, opacity:1, scale:1, rotation:0.02, ease: Elastic.easeOut.config(0.8, 0.3), delay:1.4 });

    TweenLite.fromTo( timeLine, 1.6, { time:0 },{ time:1, onComplete:animate_1});
}
function animate_1(){
    TweenLite.to( burger, 1.2, { x:0, opacity:1, rotation:0.02,ease: Elastic.easeOut.config(0.5, 0.5) });
    //
    TweenLite.to( cta_wrapper, 1, { rotation:0.02, scale:1, ease: Elastic.easeInOut.config(1.2,1), delay:0.7 });
    TweenLite.to( cta_wrapper, 0.2, { opacity:1, rotation:0, delay:1.1 });
    //
    TweenLite.fromTo( timeLine, 1.5, { time:0 },{ time:1, onComplete:animate_2 });
}
function animate_2(){
    TweenLite.to( stoerer_wrapper, 1, { rotation:0.02, scale:1, ease: Elastic.easeInOut.config(1.2,1) });
    TweenLite.to( stoerer_wrapper, 0.2, { opacity:1, rotation:0, delay:0.4});
    TweenLite.to( ile_wrapper, 0.8, { opacity:1, delay:0.8});
    //offsets wrapper y:-90 , motive1 scale:0.6 transformOrigin:"10% 0%" ,
    // TweenLite.to( cta, 0.1, { rotation:0.02, scaleX:0, ease: Power1.easeOut });
    TweenLite.fromTo( timeLine, 1.8, { time:0 }, { time:1,onComplete:animate_3 });
}
function animate_3(){
    ctaAnimate();
    //
    TweenLite.fromTo( timeLine, 2, {time:0},{ time:1, onComplete:animate_4 });
}
function animate_4(){
    //
    TweenLite.to(txt_1_wrapper, 0.2, { opacity:0, rotation:0, ease: Power0.easeIn  });
    TweenLite.to(burger_wrapper, 0.2, { opacity:0, rotation:0, ease: Power0.easeIn  });
    TweenLite.to(cta_wrapper, 0.2, {  opacity:0, rotation:0.02, ease: Power0.easeIn });
    TweenLite.to(ile_wrapper, 0.2, {  opacity:0, rotation:0.02, ease: Power0.easeIn });
    TweenLite.to(stoerer_wrapper, 0.2,{  opacity:0, rotation:0.02, ease: Power0.easeIn });
    //
    TweenLite.fromTo( timeLine, 2, {time:0},{ time:1, onComplete:endAnimation });
}

function ctaAnimate() {
    if( cta_loop < 2 ) {
      TweenLite.to(stoerer_wrapper, 0.18,{  scale:0.8, rotation:0.0, delay:0.18, ease: Power1.easeOut });
      TweenLite.to(stoerer_wrapper, 0.18,{  scale:1, rotation:0.02, ease: Power0.easeOut, delay:0.36, onComplete:ctaAnimate });
    }
    else{
    }
    cta_loop++;
}

//Banderole
function animateBanderole1(){
    TweenLite.killTweensOf([banderole_top,banderole_bottom]);
    //
    TweenLite.set([banderole_top,banderole_bottom],{x:-83});
    //
    TweenLite.to([banderole_top,banderole_bottom],2,{x:0,ease: Power0.easeNone,onComplete:animateBanderole1});
}

function endAnimation(){
    //Alles zurÃ¼cksetzen
    killAllTweens();
    cta_loop=0;
    resetAll();
    resizeAll();
    animate_0();
}
function killAllTweens(){
}

//Warten bis Content da ist und dann initialisieren
window.addEventListener("load",initAll);
function initAll(){
    //einmal initial Positionieren
    resetAll();
    resizeAll();
    container.style.visibility="visible";
    //Dann Animationen starten
    startAnimations();
}
 function startAnimations(){
    //Alles zurÃ¼cksetzen
    resetAll();
    resizeAll();
    animate_0();
}
