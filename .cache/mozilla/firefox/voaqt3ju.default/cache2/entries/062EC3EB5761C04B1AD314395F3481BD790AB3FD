//RESIZING!
function resizeingAll(){
    resizeAll();
}
function resizeAll(){
    blocked=true;
    //BuhnenW und BuhenenH setten
    //BuhnenW und BuhenenH setten
    buehnenW = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;
    if(buehnenW<120){buehnenW=120};
    buehnenH = window.innerHeight || document.documentElement.clientHeight || document.body.clientHeight;
    if(buehnenH<600){buehnenH=600};
    //ResizeValue berechnen
    resizeValue=buehnenH/helpH;
    //
    neuBannerBreite=buehnenW/resizeValue;
    TweenLite.set(banner,{width:neuBannerBreite});
    //ANPASSUNGEN DES BANNERS
    TweenLite.set(banner,{scale:resizeValue});
    //
    if(buehnenW/buehnenH < 60/600){
        positionierungBG_0();
    }
    else if(buehnenW/buehnenH < 120/600){
        positionierungBG_1();
    }
    else if(buehnenW/buehnenH < 160/600){
        positionierungBG_2();
    }
    else if(buehnenW/buehnenH < 200/600){
        positionierungBG_3();
    }
    else if(buehnenW/buehnenH < 250/600){
        positionierungBG_4();
    }
    else if(buehnenW/buehnenH < 340/600){
        positionierungBG_5();
    }
    else if(buehnenW/buehnenH < 500/600){
        positionierungBG_6();
    }
    else if(buehnenW/buehnenH < 600/600){
        positionierungBG_7();
    }
    else{
        positionierungBG_8();
    }
    //container anpassen, nervig muss man aber machen =)
    container.style.width=banner.getBoundingClientRect().width+"px";

    blocked=false;
}
/*<--------------------  Ende Resize --------------------*/
