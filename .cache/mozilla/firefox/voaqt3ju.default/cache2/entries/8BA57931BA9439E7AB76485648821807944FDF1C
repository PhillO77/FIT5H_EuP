<script>
// FUNKTIONIERT NUR PUBLIZIERT WEGEN COP
(function($){
    $.ajax({
        type: "GET",
        url: "http://www.bild.de/rssfeeds/rss4/rss4-45554522,feed=feedreader-home-seo-widget.bild.html",
        dataType: "xml"
    }).done(function(xml) {
        var index = 1;
        $(xml).find('item').each(function(){
          var title = $(this).find('title').text();
            var link = $(this).find('link').text();
            var pubDate = $(this).find('pubDate').text();
   var startTime = new Date(pubDate);
   var endTime = new Date();
   var difference = endTime.getTime() - startTime.getTime(); // This will give difference in milliseconds
   var resultInMinutes = Math.round(difference / 60000);
            if(index <= 9){
             $( ".rssFeed_seo_widget_block1" ).append( "<span class='rssFeedItem"+index+"'><p class='pubDate'>Vor "+resultInMinutes+" Minuten</p><p class='title'><a href='"+link+"' alt='"+title+"'>"+title+"</a></p></span>" );
            } else {
             $( ".rssFeed_seo_widget_block2" ).append( "<span class='rssFeedItem"+index+"'><p class='pubDate'>Vor "+resultInMinutes+" Minuten</p><p class='title'><a href='"+link+"' alt='"+title+"'>"+title+"</a></p></span>" );
            }
            index++;
            if(index === 19){
             return false;
            }
        });
    }).fail(function() {
        console.log('FAILED to Parse RSS... Try Again...');
    });
})(jQuery);
</script>
