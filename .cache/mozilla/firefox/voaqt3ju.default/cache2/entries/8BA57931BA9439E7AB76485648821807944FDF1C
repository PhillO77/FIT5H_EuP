                            <div data-skinning="aaa-teaserReihe------BITTE-NICHT-AUSBAUEN-!!!-AUF-KEINEN-FALL-AUSBAUEN-!!!" data-vr-zone="aaa-teaserReihe------BITTE-NICHT-AUSBAUEN-!!!-AUF-KEINEN-FALL-AUSBAUEN-!!!">
                            <header  class="gap"></header>
        <div class="tr">
                <!-- Teaserblock und explizit nicht im Aufmacherblock -->
    <!-- Teaserreihe -->
    <div class="misc
                      itemLast" 
                        data-vr-contentbox=""
                     data-tr-teaser-home-19="bi=19|ti=5|id=46039478">
                <style>
    #retrescoHome8346 .flex-container {
        display: flex;
        flex-flow: row wrap;
        justify-content: space-between;
    }
</style>
<section style="display: none" id="retrescoHome8346">
    <header>
        <h2>Für Sie ausgewählt</h2>
    </header>
    <div class="tr flex-container"></div>
</section>
<script>
    (function($) {
        var settings = {
            selector: '#retrescoHome8346',
            wrapper: '.flex-container',
            teaserCount: 6,
            excludeTeaserSelector: '#innerWrapper > .faux.clearfix:first a',
            template: '<div class="hentry portrait t5p2 sep"><a href="%URL%" rel="bookmark"><img class="photo" src="%IMG%" alt=""><h3 class="entry-title"><span class="kicker">%KICKER%</span><span class="headline">%HEADLINE%</span></h3></a><p class="entry-content">%TEXT%&nbsp;<span><a href="%URL%" rel="tag">mehr...</a></span></p></div>',
            scriptToAppend: '<script data-tracking="true" data-onload="de.bild.retrescoHomeTest.ready" id="rtr-p13n-loader" src="http://bild.rtrp13n.de/personalization.min.js"><\/script>'
        };
        var $ele;
        var $wrapper;
        function init() {
            if(de.bild.cmsKonfig.additional.isHome !== true) {
                return;
            }
            if(window.RtrPersonalization === undefined) {
                window.de.bild.retrescoHomeTest = window.de.bild.retrescoHomeTest || {};
                window.de.bild.retrescoHomeTest.ready = function() {
                    continueInit();
                };
                $('head').append(settings.scriptToAppend);
            }
            else {
                continueInit();
            }
        }
        function continueInit() {
            $ele = $(settings.selector);
            initTracking();
            var excludeIds = [];
            $(settings.excludeTeaserSelector).each(function() {
                var $this = jQuery(this);
                var href = $this.attr('href');
                var match = href.match(/([0-9]+).bild.html$/);
                if(match !== null && match.length === 2) {
                    excludeIds.push(match[1]);
                }
            });
            $wrapper = $ele.find(settings.wrapper);
            loadTeaser(function(number) {
                $ele.show();
                de.bild.events.publish('de.bild.mobile.clickTracking', [false, {
                    webtrekk: {
                        contentId: window.webtrekk.contentId,
                        linkId: 'retr-home-recom',
                        customClickParameter: {
                            22: 'render/elemente_' + number + '/retr-home-recom',
                        }
                    }
                }]);
                // Retresco Tracking init
                try {
                    $ele.registerRtrPersonalizationWidget('home');
                }
                catch(e) {}
                
                var VPM;
                try {
                    VPM = de.bild.globalSandBox.getViewPortManager();
                }
                catch(e) {}
                if(!VPM) {
                    return;
                }
                VPM.triggerWhenInViewport($ele, function() {
                    de.bild.events.publish('de.bild.mobile.clickTracking', [false, {
                        webtrekk: {
                            contentId: window.webtrekk.contentId,
                            linkId: 'retr-home-recom',
                            customClickParameter: {
                                22: 'view/elemente_' + number + '/retr-home-recom',
                            }
                        }
                    }]);
                }, false);
            }, excludeIds);
        }
        function initTracking() {
            trackView();
            $ele.on('click', 'a', function() {
                var href = $(this).attr('href') || '';
                trackClick(href);
            });
        }
        function trackClick(href) {
            if(!window.webtrekk) {
                return;
            }
            de.bild.events.publish('de.bild.mobile.clickTracking', [false, {
                webtrekk: {
                    contentId: window.webtrekk.contentId,
                    linkId: 'retr-home-recom',
                    customClickParameter: {
                        22: 'click/retr-home-recom',
                        30: href
                    },
                    trackOnNextPage: true
                }
            }]);
            // Tealium Event
            de.bild.events.publish('dataLayerEventTracking', ['click', {
                click_name: 'retr-home-recom',
                module_click: 'click/retr-home-recom',
                target_url: href
            }]);
        }
        function trackView() {
            if(!window.webtrekk) {
                return;
            }
            de.bild.events.publish('de.bild.mobile.clickTracking', [false, {
                webtrekk: {
                    contentId: window.webtrekk.contentId,
                    linkId: 'retr-home-recom',
                    customClickParameter: {
                        22: 'view/retr-home-recom'
                    }
                }
            }]);
            // Tealium Event
            de.bild.events.publish('dataLayerEventTracking', ['click', {
                click_name: 'retr-home-recom',
                module_click: 'view/retr-home-recom'
            }]);
        }
        function loadTeaser(cb, excludeIds) {
            window.RtrPersonalization.home({
                size: settings.teaserCount,
                excludeIds: excludeIds
            }).then(function (response) {
                var items = response.content_recommendations.recommended_items;
                if(items.length < settings.teaserCount) {
                    return;
                }
                renderTeaser(items);
                cb(items.length);
            });
        }
        function renderTeaser(response) {
            var template = '';
            for(var i = 0; i < settings.teaserCount; i++) {
                var tmp = settings.template;
                var data = response[i];
                var img = data.teaser_image_url;
                img = img.replace('{$height}', '182');
                tmp = tmp.replace(/%URL%/g, data.url);
                tmp = tmp.replace('%IMG%', img);
                tmp = tmp.replace('%KICKER%', data.kicker);
                tmp = tmp.replace('%HEADLINE%', data.headline);
                tmp = tmp.replace('%TEXT%', data.teaser_text);
                template += tmp;
            }
            $wrapper.append(template);
        }
        de.bild.events.subscribe('retrescoHomeStart', function() {
            init();
        });
        de.bild.events.publish('retrescoHomeReady');
    })(jQuery);
</script>
</div>
            </div>
            </div>
                            </section></div></div>
