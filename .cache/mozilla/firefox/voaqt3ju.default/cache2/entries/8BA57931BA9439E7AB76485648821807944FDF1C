           <script>
                   var de = de || {};
                   de.bild = de.bild || {};
                   de.bild.activityMeasurement = de.bild.activityMeasurement || {};
                   de.bild.activityMeasurement.measureAsideActivity = function(action,data)// contentId, doctype, ressort, subressort, layout, position, bam)
                   {
                       var target = webtrekk.contentGroup[1]+"/"+webtrekk.customParameter[4]+"/"+webtrekk.contentGroup[2];
                       var source = data.ressort+"/"+data.doctype+"/"+data.subressort;
                       var position = data.position;
                       if(typeof(data.bam) != "undefined")
                       {
                           position = data.position+"/"+data.bam;
                       }
                       var value = source+"/"+position+"/"+target;
                       var visibleContainer = target+"/"+data.bam;
                       if (typeof(wt) != "undefined")
                       {
                           if(action === 'click')
                           {
                               wt.sendinfo({contentId: data.contentId, linkId: "regularien",customClickParameter:{15:value,16:webtrekk.customParameter[4],17:data.layout}});
                           }
                           else if((action === 'visible') && (false) && data.bam)
                           {
                                  wt.sendinfo({contentId: webtrekk.contentId, linkId: "",customClickParameter:{18:visibleContainer}})
                           }
                       }
                   }
               </script>
