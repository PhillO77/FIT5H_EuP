<script>
    (function($) {
        var selector = '#liveticker4-bc-widget';
        var $ele = $(selector);
        // set position of upper Teaser
        var $firstTeaser = $ele.parent().parent().parent().find('.pos2');
        $firstTeaser.css('z-index', '9');
        $firstTeaser.css('top', '0px');
        // set logo href
        $ele.find('.ticker-logo-link').attr('href', $firstTeaser.find('a').attr('href').toString());
        // get Data from JSON-API and build span Elements for Date/Header in .ticker-bc-wrapper
        $.getJSON("http://json.bild.de/servlet/json/ios/39106330", function(data) {
            })
            .done(function(data){
                var markup = '';
                var i = 4;
                for (var x = 1; x <= i ; x++) {
                    var tickerURL = data['sharingURL'].toString();
                    var tickerHeader = data['text'][1]['__childNodes__'][x]['header'].toString();
                    var coremediaId = data['text'][1]['__childNodes__'][x]['coremediaId'].toString();
                    var tickerDate = data['text'][1]['__childNodes__'][x]['date'];
                    // get next Ticker Object if no header/date exists
                    if(tickerHeader === '' || tickerDate === undefined){
                        i++;
                    }
                    else {
                        tickerDate = tickerDate.toString();
                        try{
                            var hours_minutes_secs = tickerDate.split('T')[1].split(':');
                            var HH = hours_minutes_secs[0];
                            var mm = hours_minutes_secs[1];
                        } catch(err){
                            console.log(err);
                            var HH = '00';
                            var mm = '00';
                        }
                        
                        markup += '<span class="tickerDate"><a href="' + tickerURL.substr(17, tickerURL.length) + '#' +coremediaId+ '" alt="Sport Tag im Ticker"> ' + HH + ':' + mm + ' </a></span>';
                        markup += '<span class="tickerHeader"><a href="' + tickerURL.substr(17, tickerURL.length) + '#' +coremediaId+ '" alt="Sport Tag im Ticker"> ' + tickerHeader + ' </a></span>';
                    }
                };
                var $ele = $(selector);
                var $wrapper = $ele.find('.ticker-bc-wrapper');
                $wrapper.append(markup);
            })
            .fail(function(){
                console.log('get JSON fail!');
            });
    })(jQuery);
</script>
</div>
