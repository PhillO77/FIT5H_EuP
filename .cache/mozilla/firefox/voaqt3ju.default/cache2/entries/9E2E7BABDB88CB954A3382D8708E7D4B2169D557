<script src="//cdn.playbuzz.com/content/bundles/production/af3422f/pbads/dfp/js"></script>




<script>

    var UTMDomains = {".google.":"org_search",".ask.":"org_search",".yahoo.":"org_search",".bing.":"org_search",".aol.":"org_search","yandex.":"org_search","duckduckgo.com":"org_search",".pinterest.com":"org_pinterest","facebook.com":"org_facebook"};
    var UTMSponsoredArticles = {"d4b517e5-2d1f-4c03-9eef-b76424807775":"AmexCampaign","c5bd6434-3c8e-412d-b8dd-76a368de899d":"halifax","f71b8959-4bfe-48da-8df6-e1c7c6be26c2":"halifax","64426526-fe99-4050-8524-4bcb3b969f8f":"halifax"};
    var InBannerCampaignId = "352434297";
    var InQuestionCampaignIdDisregard = "none";
    var InQuestionCampaignIdDisregardArray = []; 
    var IsPBFeed = ("True" === "True");
    var dfpTargeting = {};



    if (InQuestionCampaignIdDisregard !== "none" && InQuestionCampaignIdDisregard.length>0) {
        InQuestionCampaignIdDisregardArray = InQuestionCampaignIdDisregard.split(';').map(Number); 
    }

    try {
        var sponsoredArticleIds = Object.keys(UTMSponsoredArticles);
        if (sponsoredArticleIds.indexOf(gameData.gameId) >= 0) {
            dfpTargeting["userSuvey"] = UTMSponsoredArticles[gameData.gameId];
        }
    } catch (err) {
        console.log("failed to set UTM Sponsored Articles UTM");
    }

    try {

        // abtest targeting
        for (var index = 0; index < window.adTargeting.length; ++index) {
            dfpTargeting[window.adTargeting[index].target] = window.adTargeting[index].value;
        }

        // mobile targeting
        dfpTargeting["MobileApp"] = document.location.toString().toLowerCase().search("frommobileapp=true") != -1 ? "Yes" :  "No";

        // item targeting
        if (window.gameData) {
            dfpTargeting["name"] = window.gameData.gameId;
            dfpTargeting["pattern"] = window.gameData.patternId;

            if (window.gameData.tags) {

                var dfpTags = [];
                try {
                    var predifinedTags = ["Celebrities", "Tv", "Film", "Music", "Style", "Sexy", "Cute", "Funny", "Retro", "Food", "Love", "Animals", "Holidays", "Art", "Books", "Games", "Tech", "Internet", "World", "Sports", "Politics" ,"News"];
                    var itemTags = window.gameData.tags.split(',');
                    dfpTags = itemTags.filter(function(tag) {
                        return (predifinedTags.indexOf(tag) != -1);
                    });

                } catch (e) { console.log("failed to set dfp tags targeting", e); }

            }

            dfpTargeting["tags"] = dfpTags.toString();
        }

        // utm targeting
        var pbUtmCookie = new UtmCookie();
        var utmTerm = pbUtmCookie.getTerm(),
            utmMedium = pbUtmCookie.getMedium(),
            utmSource = pbUtmCookie.getSource();
        if (utmMedium !== null) { dfpTargeting['utm_medium'] = utmMedium; }
        if (utmTerm !== null) { dfpTargeting['utm_term'] = utmTerm; }
        if (utmSource !== null) { dfpTargeting['utm_source'] = utmSource; }

        // ads module targeting
        if (window.pbads && window.pbads.loaded) {

            // set largeplayer ( = pbads module loaded)
            dfpTargeting["Largeplayer"] = "Yes";

            // set abtest
            try { var abtest = pbads.abtest.getActiveVariation();} catch (ex) { abtest = "NA"; }
            dfpTargeting["pbadsabtest"] = abtest;

        } else {
            dfpTargeting["Largeplayer"] = "No";
        }

        try {
            var taretingByUrl = Tools.getUrlParameterByName("pbads_preview");
            if (taretingByUrl != null) { dfpTargeting["preview"] = taretingByUrl }
        } catch(e) { console.log("failed set preview dfp targeting");  }

        try {
            //need to check if we have referer and according to the new rules decide if we should override the utm's?
            var ReferredURL = "www.bild.de";
            if (!Tools.getUrlParameterByName("utm_source")) {
                //check if the referer is Playbuzz or not
                if (ReferredURL.indexOf("playbuzz.com") == -1) {
                    //meaning we got to Playbuzz but not from an internal URL
                    //check if the referer domain is in predefined in UTM domains
                    //if not - reset UTM's
                    var isDomainInUTMList = false;
                    var foundDomain = "";
                    //check that we got a list of domains
                    if (typeof(UTMDomains) == "object") {
                        //get domains
                        var UTMDomainsList = Object.keys(UTMDomains);
                        for (UTMDomain in UTMDomainsList) {
                            //check if referer domain contains UTM domain
                            if (ReferredURL.indexOf(UTMDomainsList[UTMDomain]) >= 0) {
                                //if match found - save the domain and mark that it was found
                                foundDomain = UTMDomainsList[UTMDomain];
                                isDomainInUTMList = true;
                                break;
                            }
                        }
                    }
                    if (isDomainInUTMList) {
                        dfpTargeting['utm_source'] = foundDomain;
                        dfpTargeting['utm_medium'] = UTMDomains[foundDomain];
                    } else {
                        dfpTargeting['utm_source'] = "";
                        dfpTargeting['utm_medium'] = "";
                        dfpTargeting['utm_term'] = "";
                    }
                    //override UTM Cookie
                    pbUtmCookie.cookie.utm_medium = dfpTargeting['utm_medium'];
                    pbUtmCookie.cookie.utm_source = dfpTargeting['utm_source'];
                    pbUtmCookie.setBrowserCookie();
                } else {
                    //meaning this is an internal playbuzz.com navigation
                    if (!IsPBFeed) {
                        MonetizationBridge.NextPageVisit();
                    }
                }
            }
        } catch(err) {
            console.log("failed to set/reset pbads UTM targeting by Domain");
        }
        // set targeting from pbads targeting queue
        if (window.pbads && window.pbads.targeting) {
            try {
                window.pbads.targeting.forEach(function(entry) {
                    Object.keys(entry).forEach(function (key) {
                        dfpTargeting[key] = entry[key];
                    });
                });
            } catch (ex) { console.log("failed to set pbads queue targeting", ex); }
        }


    }
    catch (ex) { console.warn("failed to set dfp targeting", ex); }

</script>



<script>

    var enableAds = false;

    // initiating dfp
    try {
        var dfpUnitsConfig= "{\"path\":\"network_game_play_reveal\",\"config\":{\"ad-tags-placeholder\":{\"adunit\":\"1x1_pixel_Network\",\"dimensions\":\"1x1\"}}}";
        pbads.dfp.init('94166617', dfpTargeting, dfpUnitsConfig);

    } catch (e) {
        // log to console --> we need alert if we get this error
        console.error("FATAL: failed to load dfp module", e);
    }


    $(document).ready(function() {
        // fetch dfp ads
        try {

            if (enableAds) {
                    
                // fetch units
                pbads.dfp.fetch();

                // refresh on game progress 50
                if (window.gameData && (window.gameData.patternId === "TestYourself" || window.gameData.patternId === "multipleChoice")) {
                    $(document).on("Game.Progress.50", function() { 
                        try { 
                            //need to change the query string
                            window.location.hash = 'half'; //add half to URL
                            //googletag.pubads().refresh() 
                            pbads.dfp.refresh();
                        } catch (e) { 
                            console.warn("failed to fetch dfp adunits", e); 
                        }
                    });
                }

            } else {  
                $(".adunit-wrapper").hide();                
            }

        } catch (e) {
            // log to console --> we need alert if we get this error
            console.error("FATAL: failed to fetch dfp adunits", e);
        }
    });

</script>


    

