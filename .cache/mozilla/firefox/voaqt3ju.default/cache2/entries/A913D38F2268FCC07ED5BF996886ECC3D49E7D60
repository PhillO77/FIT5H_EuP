}
//RESETTEN
function resetAll(){
    active=2;
    //
    TweenLite.set(bannerClassArray,{force3D:true});
    TweenLite.set(banner,{rotation:0,transformOrigin:"0% 0%",force3D:true});
    //Motive
    //offsets wrapper y:-90 , motive1 scale:0.6 transformOrigin:"10% 0%"
    TweenLite.set(motive_1_container,{opacity:1,transformOrigin:"0% 0%",scale:1,force3D:true});
        TweenLite.set(motive_1_wrapper,{x:0,y:0, opacity:0,scale:0,rotation:0,transformOrigin:"50% 50%",force3D:true});
            TweenLite.set(motive_1,{x:0,y:200,opacity:1,scale:1,rotation:0,transformOrigin:"10% 0%",force3D:true});
            TweenLite.set(motive_2,{x:0,y:200,opacity:0,scale:0,rotation:0,transformOrigin:"50% 50%",force3D:true});
    //offsets wrapper y:-90 , motive1 scale:0.6 transformOrigin:"10% 0%"
    TweenLite.set(burger_container,{opacity:1,transformOrigin:"0% 0%",scale:1,force3D:true});
        TweenLite.set(burger_wrapper,{x:500,y:-50, opacity:0,scale:1,rotation:0,transformOrigin:"50% 50%",force3D:true});
            TweenLite.set(burger,{x:0,y:0,opacity:1,scale:1,rotation:0,transformOrigin:"10% 0%",force3D:true});
    //COKE
    TweenLite.set(coke_container,{opacity:1,transformOrigin:"0% 0%",scale:1,force3D:true});
        TweenLite.set(coke_wrapper,{x:-100,y:-50, opacity:0,scale:.9,rotation:0,transformOrigin:"50% 50%",force3D:true});
            TweenLite.set(coke,{x:0,y:0,opacity:1,scale:1,rotation:0,transformOrigin:"10% 0%",force3D:true});
    //TXT1
    TweenLite.set(txt_1_container,{opacity:0,rotation:0,transformOrigin:"0% 0%",force3D:true});
        TweenLite.set(txt_1_wrapper,{x:0, y:0, opacity:1,scale:1,transformOrigin:"0% 0%",force3D:true});
            TweenLite.set(txt_1_1,{x:0,y:280,opacity:1,scale:1,rotation:0,transformOrigin:"50% 50%",force3D:true});
            TweenLite.set(txt_1_3,{x:0,y:280,opacity:1,scale:1,rotation:0,transformOrigin:"50% 50%",force3D:true});
            TweenLite.set(txt_1_4,{x:0,y:280,opacity:1,scale:1,rotation:0,transformOrigin:"50% 50%",force3D:true});
    //TXT2
    TweenLite.set(txt_2_container,{opacity:0,rotation:0,transformOrigin:"0% 0%",force3D:true});
        TweenLite.set(txt_2_wrapper,{opacity:1,scale:1,transformOrigin:"0% 0%",force3D:true});
            TweenLite.set(txt_2_1,{x:0,y:0,opacity:1,scale:1,rotation:0,transformOrigin:"50% 50%",force3D:true});
            TweenLite.set(txt_2_2_1,{x:0,y:0,opacity:1,scale:1,rotation:0,transformOrigin:"50% 50%",force3D:true});
            TweenLite.set(txt_2_2_2,{x:0,y:0,opacity:1,scale:1,rotation:0,transformOrigin:"50% 50%",force3D:true});
            TweenLite.set(txt_2_3,{x:0,y:0,opacity:1,scale:1,rotation:0,transformOrigin:"50% 50%",force3D:true});
            TweenLite.set(txt_2_4,{x:0,y:0,opacity:1,scale:1,rotation:0,transformOrigin:"50% 50%",force3D:true});
    //TXT3
    TweenLite.set(txt_3_container,{opacity:0, rotation:0, transformOrigin:"50% 50%",force3D:true});
        TweenLite.set(txt_3_wrapper,{opacity:1,scale:1,transformOrigin:"50% 50%",force3D:true});
            TweenLite.set(txt_3_1,{x:0,y:0,opacity:1,scale:1,rotation:0,transformOrigin:"50% 50%",force3D:true});
            TweenLite.set(txt_3_1_2,{x:0,y:0,opacity:1,scale:1,rotation:0,transformOrigin:"50% 50%",force3D:true});
            TweenLite.set(txt_3_2,{x:0,y:0,opacity:1,scale:1,rotation:0,transformOrigin:"50% 50%",force3D:true});
            TweenLite.set(txt_3_3,{x:0,y:0,opacity:1,scale:1,rotation:0,transformOrigin:"50% 50%",force3D:true});
            TweenLite.set(txt_3_4,{x:0,y:0,opacity:1,scale:1,rotation:0,transformOrigin:"50% 50%",force3D:true});
            TweenLite.set(txt_3_5,{x:0,y:0,opacity:1,scale:1,rotation:0,transformOrigin:"50% 50%",force3D:true});
    //CTA
    TweenLite.set(cta_container,{opacity:0, scale:1, transformOrigin:"0% 0%", force3D:true});
        TweenLite.set(cta_wrapper,{opacity:1,rotation:0,scale:1, transformOrigin:"0% 0%", force3D:true});
          TweenLite.set(cta_wide,{opacity:1,rotation:0,transformOrigin:"50% 50%",force3D:true});
          TweenLite.set(cta_small,{opacity:1,rotation:0,transformOrigin:"50% 50%",force3D:true});
    //LOGO
    TweenLite.set(logo_container,{opacity:1,transformOrigin:"100% 0%",force3D:true});
    TweenLite.set(logo,{opacity:1,force3D:true});
    //CLICKTHROUGH
    TweenLite.set(clickThrough,{opacity:0});
}
//ANIMATIONEN
function animate_0(){
    //
    TweenLite.to(motive_1_wrapper,1.5,{scale:1,rotation:0.02,ease: Elastic.easeOut.config(1, 0.3), delay:0.5});
    TweenLite.to(motive_1_wrapper,0.2,{opacity:1, delay:0.5});
    TweenLite.fromTo(timeLine,1.5,{time:0},{time:1,onComplete:animate_1});
}
function animate_1(){
    //offsets wrapper y:-90 , motive1 scale:0.6 transformOrigin:"10% 0%" ,
    TweenLite.to(motive_1, 0.2, {rotation:0.02, scale:0.6 });
    TweenLite.to(motive_2, 1.5, {scale:1.2, rotation:0.02, ease: Elastic.easeOut.config(1, 0.3) });
    TweenLite.to(motive_2, 0.2, {opacity:1 });
    TweenLite.fromTo(timeLine, 1.5, {time:0}, {time:1,onComplete:animate_2});
}
function animate_2(){
    TweenLite.to(motive_1_wrapper,0.3,{rotation:0,y:-90});
    TweenLite.from(txt_1_1,0.3,{opacity:0,rotation:0.02,y:500,ease: Back.easeOut.config(1.7),delay:0});
    TweenLite.from(txt_1_3,0.3,{opacity:0,rotation:0.02,y:500,ease: Back.easeOut.config(1.7),delay:0.2});
    TweenLite.from(txt_1_4,0.3,{opacity:0,rotation:0.02,y:500,ease: Back.easeOut.config(1.7),delay:0.3});
    //
    TweenLite.set(txt_1_container,{opacity:1, rotation:0.02});
    //
    TweenLite.fromTo( timeLine, 1.7, {time:0},{ time:1, onComplete:animate_3 });
}
function animate_3(){
    TweenLite.to(burger_wrapper,1.2,{scale:1, x:0, rotation:0.02,ease: Elastic.easeOut.config(0.7, 0.3), delay:0.2});
    TweenLite.to(burger_wrapper,0.2,{opacity:1, delay:0.2});
    //
    TweenLite.to(motive_1_wrapper,0.8, { x:-20, y:-370, rotation:0.02, scale:0.55, ease: Elastic.easeOut.config(0.6, 0.6), delay:0.2});
    TweenLite.to(txt_1_wrapper,0.2, { x:-200, y:0, opacity:0, rotation:0.02, scale:1, ease: Power2.easeIn});

    TweenLite.fromTo( timeLine, 0.3, {time:0},{ time:1, onComplete:animate_4 });
}
function animate_4(){
    //
    TweenLite.set(txt_2_container,{opacity:1, rotation:0.02});
    //
    TweenLite.from(txt_2_1,1.3,{opacity:0,rotation:0.02,scale:0,ease: Elastic.easeOut.config(1, 0.4),delay:0.1});
    TweenLite.from(txt_2_2_1,1.3,{opacity:0,rotation:0.02,scale:0,ease: Elastic.easeOut.config(1, 0.4),delay:0.2});
    TweenLite.from(txt_2_2_2,1.3,{opacity:0,rotation:0.02,scale:0,ease: Elastic.easeOut.config(1, 0.4),delay:0.3});
    TweenLite.from(txt_2_3,1.3,{opacity:0,rotation:0.02,scale:0,ease: Elastic.easeOut.config(1, 0.4),delay:0.4});
    TweenLite.from(txt_2_4,1.3,{opacity:0,rotation:0.02,scale:0,ease: Elastic.easeOut.config(1, 0.4),delay:0.5});
    //
    TweenLite.fromTo(timeLine, 2.5, {time:0},{ time:1, onComplete:animate_5 });
}
function animate_5(){
    TweenLite.to( txt_2_container, 0.5,{ opacity:0, rotation:0 });
    TweenLite.to( txt_3_container,0.6,{ opacity:1, rotation:0.02 });
    //
    TweenLite.from(txt_3_1,1.3,{opacity:0,rotation:0.02,scale:0,ease: Elastic.easeOut.config(1, 0.4),delay:0.5});
    TweenLite.from(txt_3_1_2,1.3,{opacity:0,rotation:0.02,scale:0,ease: Elastic.easeOut.config(1, 0.4),delay:0.5});
    TweenLite.from(txt_3_2,1.3,{opacity:0,rotation:0.02,scale:0,ease: Elastic.easeOut.config(1, 0.4),delay:0.5});
    TweenLite.from(txt_3_3,1.3,{opacity:0,rotation:0.02,scale:0,ease: Elastic.easeOut.config(1, 0.4),delay:0.5});
    TweenLite.from(txt_3_4,1.3,{opacity:0,rotation:0.02,scale:0,ease: Elastic.easeOut.config(1, 0.4),delay:0.5});
    TweenLite.from(txt_3_5,1.3,{opacity:0,rotation:0.02,scale:0,ease: Elastic.easeOut.config(1, 0.4),delay:0.5});
    //
    TweenLite.to(coke_wrapper,1.1,{scale:1, x:0, y:-20, rotation:0.02,ease: Elastic.easeOut.config(0.5, 0.5),delay:0.4});
    TweenLite.to(coke_wrapper,0.3,{opacity:1, delay:0.4});

    TweenLite.to(motive_1_wrapper,0.2,{opacity:0, rotation:0});
    //
    TweenLite.fromTo(timeLine, 2,{ time:0}, {time:1, onComplete:animate_6});
}
function animate_6(){

    //
    TweenLite.set(cta_container,{opacity:1, delay:0.2})
    TweenLite.fromTo(cta_wrapper,0.3,{scale:0, opacity:0, rotation:0.02},{scale:1.3,opacity:1, rotation:0, delay:0.2});
    TweenLite.to(cta_wrapper,0.2,{scale:1, rotation:0.02, delay:0.5});
    //
    TweenLite.fromTo(timeLine, 1.6,{ time:0}, {time:1, onComplete:animate_7});
}
function animate_7(){
    //
    ctaAnimate();
    //
    TweenLite.fromTo(timeLine, 2.5,{ time:0}, {time:1, onComplete:animate_8});
}
function animate_8(){
    //
    TweenLite.to(txt_3_container,0.4,{opacity:0, rotation:0});
    TweenLite.to(coke_wrapper, 0.4,{opacity:0, rotation:0, delay:0.2});
    TweenLite.to(burger_wrapper,0.4,{opacity:0, rotation:0, delay:0.2});
    TweenLite.to(cta_wrapper,0.4,{opacity:0, rotation:0, delay:0.2});
    //
    TweenLite.fromTo(timeLine, 1,{ time:0}, {time:1, onComplete:endAnimation});
}

function ctaAnimate(){
    if(cta_loop < 3){
      TweenLite.to(cta_wrapper, 0.2,{ scale:1.2, rotation:0, Ease: Back.easeOut.config(1)});
      TweenLite.to(cta_wrapper, 0.2,{ scale:1, rotation:0.02, Ease: Back.easeOut.config(1), delay:0.2, onComplete:ctaAnimate});
    }
    else{}
    cta_loop++;
}

function endAnimation(){
    //Alles zurÃ¼cksetzen
    killAllTweens();
    cta_loop=0;
    resetAll();
    resizeAll();
    animate_0();
}
function killAllTweens(){
}
//SLIDER
  function addingEvents(){
    clickThrough.addEventListener("click",clickhandler);
    try{
        myFT.on("sizechange",resizeingAll);
    }
    catch(err){
    }
  }
  function cta_1_Handler(){
    myFT.tracker("denim_guide_clicked");
    cta_1_wrapper.removeEventListener("click", cta_1_Handler,false);
    animate_2();
  }
//clickTag

function clickhandler(event){
    myFT.clickTag(1);
}
//Warten bis Content da ist und dann initialisieren
window.addEventListener("load",initAll);
function initAll(){
    //einmal initial Positionieren
    addingEvents();
    resetAll();
    resizeAll();
    container.style.visibility="visible";
    //Dann Animationen starten
    startAnimations();
}
 function startAnimations(){
    //Alles zurÃ¼cksetzen
    resetAll();
    resizeAll();
    animate_0();
}
