function change_txt_1(){
  TweenLite.set(txt_1_1,{left: -250,top: 0});
  TweenLite.set(txt_1_3,{left: -80,top: 125});
  TweenLite.set(txt_1_4,{left: 120,top: 90});
}

function change_txt1_wide(){
  TweenLite.set(txt_1_1,{left: -330,top: 0});
  TweenLite.set(txt_1_3,{left: 0,top: 80});
  TweenLite.set(txt_1_4,{left: 200,top: 50});
}
function change_txt_2(){
