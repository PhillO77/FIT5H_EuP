//RESIZING!
function resizeingAll(){
    resizeAll();
}
function resizeAll(){
    blocked=true;
    //BuhnenW und BuhenenH setten
    //BuhnenW und BuhenenH setten
    buehnenW = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;
    if(buehnenW<100){buehnenW=100};
    buehnenH = window.innerHeight || document.documentElement.clientHeight || document.body.clientHeight;
    if(buehnenH<600){buehnenH=600};
    //ResizeValue berechnen
    resizeValue=buehnenH/helpH;
    //
    neuBannerBreite=buehnenW/resizeValue;
    TweenLite.set(banner,{width:neuBannerBreite});
    //ANPASSUNGEN DES BANNERS
    TweenLite.set(banner,{scale:resizeValue});
    //
    if(buehnenW/buehnenH < 70/600){
        positionierungBG_0();
    }
    else if(buehnenW/buehnenH < 120/600){
        positionierungBG_1();
    }
    else if(buehnenW/buehnenH < 160/600){
        positionierungBG_2();
    }
    else if(buehnenW/buehnenH < 200/600){
        positionierungBG_3();
    }
    else if(buehnenW/buehnenH < 250/600){
        positionierungBG_4();
    }
    else if(buehnenW/buehnenH < 340/600){
        positionierungBG_5();
    }
    else if(buehnenW/buehnenH < 500/600){
        positionierungBG_6();
    }
    else if(buehnenW/buehnenH < 600/600){
        positionierungBG_7();
    }
    else{
        positionierungBG_8();
    }
    //container anpassen, nervig muss man aber machen =)
    container.style.width=banner.getBoundingClientRect().width+"px";

    blocked=false;
}
/*<--------------------  Ende Resize --------------------*/
//POSITIONINGS

function positionierungBG_0(){
  //< 90/600
  txt_3_1_2.style.display="block";
  cta_small.style.display="block";
  cta_wide.style.display="none";
  //MOTIVE
  TweenLite.set(motive_1_container,{left:"55%",top:"25%",scale:0.18});
  //BURGER
  TweenLite.set(burger_container,{left:"80%",top:"65%",scale:0.45});
  //TXT1
  TweenLite.set(txt_1_container,{left:"52%",top:"30%",scale:0.22});
  //TXT2
  TweenLite.set(txt_2_container,{left:"65%",top:"36%",scale:0.28});
  //TXT3
  TweenLite.set(txt_3_container,{left:"70%",top:"8%",scale:0.25});
  //COKE
  TweenLite.set(coke_container,{left:"75%",top:"43%",scale:0.95});
  //CTA
  TweenLite.set(cta_container,{left:"50%",top:"28%",scale:.35});
  //LOGO
  TweenLite.set(logo_container,{scale:.7});
  change_txt_1();
  change_txt_2();
  change_txt_3();
}
