                <h1 itemprop="name"><a href="/questions/9202107/class-type-checking" class="question-hyperlink">Class (Type) checking</a></h1>
		</div>
			<div id="mainbar">

			

<div class="question" data-questionid="9202107"  id="question">

    <script>
                var ados = ados || {}; ados.run = ados.run || [];
