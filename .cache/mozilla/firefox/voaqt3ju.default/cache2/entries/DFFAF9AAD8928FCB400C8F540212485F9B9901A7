    <script>
(function($) {
    //Social button tracking
    $('a.s1-social-button').on('click touchend', function( event ) {
        if( window.ga ) {
            var socialButtonClass = $(event.target).attr("class").match(/s1-social-button-[\w-]*\b/);
            window.ga("send", "event", "socialButtons", event.type, socialButtonClass.toString());
        }
    });

    $('a.s1-live-ticker-refresh').on( 'click touchend', function( event ) {
        var matches = location.search.match(/match-id=(\d+)/),
            matchId = matches && matches.length > 0 ? '/'+matches[1] : '';

        if( window.ga ){
            window.ga('send', 'event', {
                'eventCategory': 'Ticker',
                'eventLabel': location.pathname + matchId,
                'eventAction': "refresh"
            });
        }
    });

    jQuery('#wfb-tabcalc-refresh').click(function () {
        dataLayer.push({
            event: 'ga_event',
            eventAction: 'Tabellenrechner',
            eventCategory: location.pathname,
            eventLabel: 'Refresh'
        });
    });
    jQuery('#wfb-tabcalc-prev-matchday').click(function () {
        dataLayer.push({
            event: 'ga_event',
            eventAction: 'Tabellenrechner',
            eventCategory: location.pathname,
            eventLabel: 'backward'
        });
    });
    jQuery('#wfb-tabcalc-next-matchday').click(function () {
        dataLayer.push({
            event: 'ga_event',
            eventAction: 'Tabellenrechner',
            eventCategory: location.pathname,
            eventLabel: 'forward'
        });
    });
    jQuery('.wfb-tabcalc-navi-day').click(function () {
        dataLayer.push({
            event: 'ga_event',
            eventAction: 'Tabellenrechner',
            eventCategory: location.pathname,
            eventLabel: 'matchday'
        });
    });
})(jQuery);
</script>

