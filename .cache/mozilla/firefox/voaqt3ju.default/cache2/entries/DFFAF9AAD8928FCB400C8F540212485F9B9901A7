                    </a></div></div></div></div></div><script>
(function () {
    "use strict";

    if (window.s1IsMobileBrowser || window.s1IsMobileDomain) {
        return;
    }

    var interval = 5 * 6e4,
        lastId,
        layer = jQuery("#s1-reload"),
        loop,
        later,
        google,
        hide,
        reload;

    google = function (action) {
        if (window.ga) {
            window.ga("send", "event", "UpdateLayer", action, location.pathname);
        }
    };

    later = function () {
        setTimeout(loop, interval);
    };

    hide = function () {
        layer.addClass("hidden");
        later();
        window.ivwPI();
        google("cancel");
    };

    reload = function () {
        google("reload");
        // wait for the tracking to go
        setTimeout(function () {
            location.reload();
        }, 200);
    };

    loop = function () {
        jQuery
            .ajax("/api/news/count/5%20minutes%20ago")
            .success(function (data) {
                var hasNews = data && data.result && data.result > 0;
                if (!hasNews) {
                    later();
                } else {
                    window.ivwPI();
                    google("show");
                    layer.removeClass("hidden");
                }
            })
            .error(later);
    };

    later();

    layer
        .delegate(".cancel, .cancel_x", "click", hide)
        .delegate(".reload", "click", reload);
}());
</script><div data-vr-zone="Dashboard Home: Position 20"   class="s1-dashboard default-dashboard" ng-module="genericWidgetApp"><div class="s1-centered s1-background"><div class="s1-dashboard-headline"><h2 class="s1-dashboard-channel">Mehr Sport</h2></div><div class="s1-dashboard-main"><div class="s1-dashboard-teasers"><a
