        (function () {
        var articleName = '';

        window.PlayBuzzCallback = function (event) {
            var eventName = event.eventName;

            switch (eventName) {
                case 'articleLoaded':
                    articleName = event.data.articleName;

                    if (window.ga) window.ga('send', 'event', {
                        'eventCategory': 'Playbuzz',
                        'eventAction': 'view',
                        'eventLabel': articleName
                    });
                    break;
                case 'ClickPlay':
                    if (ivwPI) ivwPI();
                    if (window.ga) window.ga('send', 'event', {
                        'eventCategory': 'Playbuzz',
                        'eventAction': 'click',
                        'eventLabel': articleName
                    });
                    break;
                case 'itemResult':
                    if (ivwPI) ivwPI();
                    break;
            }
        }
    }());

