    if (nuggadios) {
        // Load Mobile Solutions Lib on iOS
        document.write('<scr' + 'ipt type="text/javascript" src="'+nugghost+'/javascripts/nuggad-ls.js"></scr' + 'ipt>');
    }
    else {
        // Sync RC-Call for all non-iOS Devices (incl Desktop)
